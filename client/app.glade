<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk+" version="3.12"></requires>
  <object class="GtkListStore" id="result_store">
    <columns>
      <column type="gchararray"></column>
      <column type="gchararray"></column>
      <column type="gchararray"></column>
    </columns>
  </object>
   <object class="GtkWindow" id="window_control">
    <property name="can_focus">False</property>
    <property name="width_request">600</property>
    <property name="height_request">400</property>
    <property name="transient_for">window_main</property>
    <signal handler="on_window_contorol_key_release" name="key-release-event" swapped="no" />
    <signal handler="on_window_contorol_delete" name="delete-event" swapped="no" />
    <child>
      <object class="GtkNotebook" id="notebook">
        <property name="visible">True</property>
        <property name="can_focus">True</property>
        <child>
          <object class="GtkBox" id="result_box">
            <property name="can_focus">False</property>
            <property name="orientation">vertical</property>
            <property name="visible">True</property>
            <child>
              <object class="GtkTreeView" id="result_tree">
                <property name="can_focus">True</property>
                <property name="model">result_store</property>
                <property name="visible">True</property>
                <child internal-child="selection">
                  <object class="GtkTreeSelection" id="treeview-selection1"></object>
                </child>
                <child>
                  <object class="GtkTreeViewColumn">
                    <property name="title" translatable="yes">column</property>
                    <child>
                      <object class="GtkCellRendererText" id="cellrenderertext1"></object>
                      <attributes>
                        <attribute name="text">0</attribute>
                      </attributes>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="GtkTreeViewColumn">
                    <property name="title" translatable="yes">column</property>
                    <child>
                      <object class="GtkCellRendererText"></object>
                      <attributes>
                        <attribute name="text">1</attribute>
                      </attributes>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="GtkTreeViewColumn">
                    <property name="title" translatable="yes">column</property>
                    <child>
                      <object class="GtkCellRendererText"></object>
                      <attributes>
                        <attribute name="text">2</attribute>
                      </attributes>
                    </child>
                  </object>
                </child>
              </object>
              <packing>
                <property name="expand">False</property>
                <property name="fill">True</property>
                <property name="position">0</property>
              </packing>
            </child>
          </object>
        </child>
        <child type="tab">
          <object class="GtkLabel">
            <property name="visible">True</property>
            <property name="label" translatable="yes">tab 1</property>
          </object>
          <packing>
            <property name="tab_fill">False</property>
          </packing>
        </child>
        <child>
          <object class="GtkBox">
            <property name="visible">True</property>
            <property name="can_focus">False</property>
            <property name="orientation">vertical</property>
            <child>
              <object class="GtkLabel">
                <property name="visible">True</property>
                <property name="label" translatable="yes">TAB 2 CONTENT</property>
              </object>
            </child>
          </object>
        </child>
        <child type="tab">
          <object class="GtkLabel">
            <property name="visible">True</property>
            <property name="label" translatable="yes">tab 2</property>
          </object>
          <packing></packing>
        </child>
      </object>
    </child>
  </object>
  <object class="GtkWindow" id="window_main">
    <property name="can_focus">False</property>
    <property name="width_request">800</property>
    <property name="height_request">500</property>
    <signal name="delete-event" handler="on_window_main_delete" swapped="no" />
    <signal name="button-press-event" handler="on_window_main_click" swapped="no" />
    <child>
      <object class="GtkMenu" id="context_menu">
        <child>
          <object class="GtkMenuItem">
            <property name="visible">True</property>
            <property name="label" translatable="yes">Analyze</property>
            <signal name="activate" handler="on_menu_item_analyze_activate" swapped="no"/>
          </object>
        </child>
        <child>
          <object class="GtkCheckMenuItem">
            <property name="visible">True</property>
            <property name="active">True</property>
            <property name="label" translatable="yes">Show status</property>
            <signal name="activate" handler="on_menu_item_status_toggler_activate" swapped="no"/>
          </object>
        </child>
        <child>
          <object class="GtkSeparatorMenuItem">
            <property name="visible">True</property>
          </object>
        </child>
        <child>
          <object class="GtkMenuItem">
            <property name="visible">True</property>
            <property name="label" translatable="yes">Quit</property>
            <signal name="activate" handler="on_menu_item_quit_activate" swapped="no"/>
          </object>
        </child>
      </object>
      <object class="GtkOverlay" id="container1">
        <child type="overlay">
          <object class="GtkImage" id="image">
            <property name="stock">gtk-missing-image</property>
          </object>
        </child>
        <child type="overlay">
          <object class="GtkBox" id="box">
            <property name="width_request">200</property>
            <property name="spacing">20</property>
            <property name="orientation">vertical</property>
            <property name="margin_top">20</property>
            <property name="margin_right">20</property>
            <child>
              <object class="GtkLabel" id="label_status">
                <property name="label" translatable="yes">Initializing...</property>
                <property name="width_request">200</property>
                <attributes>
                  <attribute name="weight" value="PANGO_WEIGHT_BOLD"></attribute>
                  <attribute name="background" value="gray"></attribute>
                </attributes>
              </object>
              <packing>
                <property name="pack_type">end</property>
              </packing>
            </child>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
